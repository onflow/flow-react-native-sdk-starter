{
  "expo": {
    "name": "flow-react-native-sdk-starter",
    "slug": "flow-react-native-sdk-starter",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "scheme": "flowreactnativesdkstarter",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.flowreactnativesdkstarter"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#00EF8B",
        "foregroundImage": "./assets/app-icon.png"
      },
      "package": "com.anonymous.flowreactnativesdkstarter"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
